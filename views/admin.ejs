                            <div id="id" class="" style="display: none;">
                                <label class="form-container__label" for="title">ID</label>
                                <input id="idArt" class="form-container__input input-form" readonly type="text"
                                    name="artID">
                            </div>

                            <div id="art-name-container" class="name-container form-container__item">
                                <label class="form-container__label" for="title">Nombre del artículo</label>
