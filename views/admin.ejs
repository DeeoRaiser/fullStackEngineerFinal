                                </div>
                                <input id="routeImageArt" class="form-container__input input-form button__ok"
                                    type="file" accept="image/*" name="image" required>

                            </div>

                            <div id="art-date-container" class="date-container form-container__item">
                                <label class="form-container__label" for="date-date">Descripcion del Artículo</label>
                                <textarea class="form-container__textarea textarea-form" name="description"
                                    required></textarea>
                            </div>

                            <div id="art-price-container" class="price-container form-container__item">
                                <label class="form-container__label" for="price">Precio</label>
                                <input class="form-container__input input-form" type="text" name="price"
                                    placeholder="1000,00" required>
                            </div>

                            <div id="art-date-container" class="date-container form-container__item">
                                <label class="form-container__label" for="date-date">Fecha</label>
                                <input class="form-container__input input-form" type="date" name="date" required>
                            </div>

                            <div class="submit">
                                <input id="art-cancel" class="submit__button-cancel button__cancel" value="Cancelar"
                                    type="button">
                                <input id="art-submit" class="submit__button-save button__ok" value="Actualizar"
                                    type="submit">
                            </div>
                        </form>
